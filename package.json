{
  "name": "gsf-js-client-sdk",
  "version": "1.0.3",
  "description": "GSF JavaScript Client SDK",
  "author": "Exelis Visual Information Solutions, Inc., a subsidiary of Harris Corporation.",
  "license": "MIT",
  "main": "src/GSF.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/geospatial-services-framework"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "build-dev": "./node_modules/.bin/webpack --progress --colors --env dev",
    "build-all": "npm run build && npm run build-dev",
    "watch": "./node_modules/.bin/webpack --progress --colors --watch",
    "test": "npm run test-phantom",
    "test-phantom": "./node_modules/.bin/karma start --browsers PhantomJS",
    "test-chrome": "./node_modules/.bin/karma start --browsers Chrome",
    "test-firefox": "./node_modules/.bin/karma start --browsers Firefox",
    "test-all": "./node_modules/.bin/karma start --browsers PhantomJS,Chrome,Firefox",
    "unit-test": "npm run unit-test-phantom",
    "unit-test-phantom": "concurrently --kill-others --success first \"node ./test/fake-server/server.js\" \"npm run test-phantom\"",
    "unit-test-chrome": "concurrently --kill-others --success first \"node ./test/fake-server/server.js\" \"npm run test-chrome\"",
    "unit-test-firefox": "concurrently --kill-others --success first \"node ./test/fake-server/server.js\" \"npm run test-firefox\"",
    "unit-test-all": "concurrently --kill-others --success first \"node ./test/fake-server/server.js\" \"npm run test-all\"",
    "help": "./node_modules/.bin/esdoc"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "superagent-no-cache": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "body-parser": "^1.17.0",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "cors": "^2.8.1",
    "esdoc": "^0.5.2",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "express": "^4.15.2",
    "istanbul": "^0.4.5",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "sse-channel": "^2.0.5",
    "superagent": "^3.3.2",
    "webpack": "^2.2.1"
  }
}
