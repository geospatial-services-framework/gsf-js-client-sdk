{
  "source": "./src",
  "destination": "./help",
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^src/", "to": "dist/"}
        ]
      }
    }
  ],
  "title": "GSF JavaScript Client SDK",
  "styles": ["./doc/styles/main.css"],
  "includeSource": true,
  "index": "./doc/index.md",
  "manual": {
    "asset": "./doc/asset",
    "installation": ["./doc/manual/installation.md"],
    "tutorial": [
      "./doc/manual/tutorial/migration-guide.md",
      "./doc/manual/tutorial/getting-started.md",
      "./doc/manual/tutorial/examples.md"
    ],
    "changelog": ["./CHANGELOG.md"]
  }
}
